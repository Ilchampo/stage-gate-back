{
  "name": "stage-gate-back",
  "version": "1.0.0",
  "description": "Manage your software project releases with ease",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "npm run prisma && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prepare": "husky",
    "test": "jest",
    "prisma": "npx prisma generate --schema=./prisma/schema.prisma",
    "dev": "nodemon -L --watch './src/**/*.ts' --exec \"ts-node\" ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ilchampo/stage-gate-back.git"
  },
  "keywords": [
    "code",
    "release",
    "manage",
    "project",
    "reviewers"
  ],
  "author": "Pablo Beltran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ilchampo/stage-gate-back/issues"
  },
  "homepage": "https://github.com/Ilchampo/stage-gate-back#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@prisma/client": "^5.17.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.0.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  }
}
